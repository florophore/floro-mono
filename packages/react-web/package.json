    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^1.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "nodemon": "^2.0.19",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sirv": "^2.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.14",
    "vite-plugin-ssr": "^0.4.18"
  }
