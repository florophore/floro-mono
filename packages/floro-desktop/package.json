{
  "name": "@floro/floro-desktop",
  "description": "Desktop client for floro",
  "homepage": "https://floro.io",
  "version": "1.0.0",
  "private": true,
  "author": {
    "email": "jamie.sunderland@floro.io",
    "name": "Cheqout Payments Inc"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.mjs",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue",
    "postinstall:manual": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,vue,json}\""
  },
  "devDependencies": {
    "@electron/notarize": "2.1.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/js-cookie": "^3.0.2",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/node": "18.17.17",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@vitejs/plugin-react": "2.0.1",
    "cross-env": "7.0.3",
    "electron": "26.2.1",
    "electron-builder": "24.6.4",
    "eslint": "8.49.0",
    "eslint-plugin-vue": "9.17.0",
    "happy-dom": "12.0.1",
    "nano-staged": "0.8.0",
    "playwright": "1.38.0",
    "rollup-plugin-node-externals": "^6.1.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "simple-git-hooks": "2.9.0",
    "typescript": "5.2.2",
    "unplugin-auto-expose": "0.0.5",
    "vite": "4.4.9",
    "vitest": "0.34.4",
    "vue": "3.3.4",
    "vue-tsc": "1.8.11"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@apollo/react-hooks": "^4.0.0",
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@tpp/pm2": "^2.1.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "apollo-upload-client": "^17.0.0",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "electron-updater": "6.1.4",
    "esbuild-plugins-node-modules-polyfill": "^1.4.0",
    "floro": "0.0.9",
    "framer-motion": "^10.10.0",
    "http-terminator": "^3.2.0",
    "isomorphic-fetch": "^3.0.0",
    "js-cookie": "^3.0.1",
    "kill-port": "^2.0.1",
    "mixpanel-browser": "^2.45.0",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.5",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.7.2",
    "whatwg-fetch": "^3.6.2"
  }
}
