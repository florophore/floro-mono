{
  "version": "0.0.2",
  "name": "palette",
  "displayName": "Palette",
  "description": "Simple plugin to organize colors.",
  "icon": {
    "light": "./palette-plugin-icon.svg",
    "dark": "./palette-plugin-icon.svg"
  },
  "imports": {},
  "types": {
    "Color": {
      "name": {
        "type": "string",
        "isKey": true
      },
      "hexcode": {
        "type": "string"
      }
    },
    "Opacity": {
      "percentage": {
        "type": "int",
        "isKey": true
      }
    }
  },
  "store": {
    "colors": {
      "type": "set",
      "values": "Color"
    },
    "opacities": {
      "type": "set",
      "values": "Opacity"
    }
  }
}