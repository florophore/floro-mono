{
  "version": "0.0.0",
  "name": "palette",
  "displayName": "palette",
  "icon": {
    "light": "./palette-plugin-icon.svg",
    "dark": "./palette-plugin-icon.svg"
  },
  "imports": {},
  "types": {
    "Color": {
      "id": {
        "isKey": true,
        "type": "string"
      },
      "name": {
        "type": "string"
      }
    },
    "Shade": {
      "id": {
        "isKey": true,
        "type": "string"
      },
      "name": {
        "type": "string"
      }
    }
  },
  "store": {
    "shades": {
      "type": "set",
      "emptyable": false,
      "values": "Shade"
    },
    "colors": {
      "type": "set",
      "emptyable": false,
      "values": "Color"
    },
    "palette": {
      "type": "set",
      "values": {
        "id": {
          "isKey": true,
          "type": "ref<$.colors.values>"
        },
        "colors": {
          "type": "set",
          "values": {
            "id": {
              "isKey": true,
              "type": "ref<$.shades.values>"
            },
            "hexcode": {
              "nullable": true,
              "type": "string"
            },
            "alpha": {
              "nullable": true,
              "type": "int"
            }
          }
        }
      }
    }
  },
  "seed": {
    "shades": [
      {
        "id": "light",
        "name": "Light"
      },
      {
        "id": "regular",
        "name": "Regular"
      },
      {
        "id": "dark",
        "name": "Dark"
      }
    ],
    "colors": [
      {
        "id": "white",
        "name": "White"
      },
      {
        "id": "black",
        "name": "Black"
      },
      {
        "id": "red",
        "name": "Red"
      },
      {
        "id": "green",
        "name": "Green"
      },
      {
        "id": "blue",
        "name": "Blue"
      }
    ],
    "palette": [
      {
        "colors": [
          {
            "alpha": 255,
            "id": "$(palette).shades.id<light>"
          },
          {
            "alpha": 255,
            "hexcode": "#FFFFFF",
            "id": "$(palette).shades.id<regular>"
          },
          {
            "alpha": 255,
            "id": "$(palette).shades.id<dark>"
          }
        ],
        "id": "$(palette).colors.id<white>"
      },
      {
        "colors": [
          {
            "alpha": 255,
            "id": "$(palette).shades.id<light>"
          },
          {
            "alpha": 255,
            "hexcode": "#000000",
            "id": "$(palette).shades.id<regular>"
          },
          {
            "alpha": 255,
            "id": "$(palette).shades.id<dark>"
          }
        ],
        "id": "$(palette).colors.id<black>"
      },
      {
        "colors": [
          {
            "alpha": 255,
            "hexcode": "#F93D44",
            "id": "$(palette).shades.id<light>"
          },
          {
            "alpha": 255,
            "hexcode": "#CC2F35",
            "id": "$(palette).shades.id<regular>"
          },
          {
            "alpha": 255,
            "hexcode": "#AA2227",
            "id": "$(palette).shades.id<dark>"
          }
        ],
        "id": "$(palette).colors.id<red>"
      },
      {
        "colors": [
          {
            "alpha": 255,
            "hexcode": "#77F075",
            "id": "$(palette).shades.id<light>"
          },
          {
