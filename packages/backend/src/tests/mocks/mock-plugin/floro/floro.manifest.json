{
  "version": "0.0.0",
  "name": "palette",
  "displayName": "Palette",
  "description": "A plugin for choosing colors.",
  "icon": {
    "light": "./palette-plugin-icon.svg",
    "dark": "./palette-plugin-icon.svg"
  },
  "imports": {},
  "types": {
    "color": {
      "name": {
        "type": "string",
        "isKey": true
      },
      "hexcode": {
        "type": "string"
      },
      "loopedType": {
        "type": "set",
        "values": "opacity"
      }
    },
    "opacity": {
      "percentage": {
        "type": "int",
        "isKey": true
      },
      "hexcode": {
        "type": "string"
      }
    }
  },
  "store": {
    "colors": {
      "type": "set",
      "values": "color"
    },
    "opacities": {
      "type": "set",
      "values": "opacity"
    }
  }
}