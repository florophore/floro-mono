{
  "version": "0.0.40",
  "name": "icons",
  "displayName": "Icons",
  "description": "A plugin for mapping colors to icons.",
  "managedCopy": true,
  "icon": {
    "light": "./icons_icon.unselected.light.svg",
    "dark": "./icons_icon.unselected.dark.svg",
    "selected": {
      "light": "./icons_icon.selected.light.svg",
      "dark": "./icons_icon.selected.dark.svg"
    }
  },
  "imports": {
    "theme": "0.0.40",
    "palette": "0.0.39"
  },
  "types": {
    "Icon": {
      "id": {
        "type": "string",
        "isKey": true
      },
      "name": {
        "type": "string"
      },
      "svg": {
        "type": "file"
      },
      "defaultIconTheme": {
        "type": "ref<$(theme).themes.values>",
        "onDelete": "nullify",
        "nullable": false
      },
      "enabledVariants": {
        "type": "set",
        "bounded": true,
        "values": {
          "id": {
            "isKey": true,
            "type": "ref<$(theme).stateVariants.values>"
          },
          "enabled": {
            "type": "boolean",
            "default": false
          }
        }
      },
      "appliedThemes": {
        "type": "set",
        "values": {
          "hexcode": {
            "isKey": true,
            "type": "string"
          },
          "themeDefinition": {
            "type": "ref<$(theme).themeColors.values>",
            "onDelete": "delete"
          }
        }
      }
    },
    "IconGroup": {
      "id": {
        "type": "string",
        "isKey": true
      },
      "name": {
        "type": "string"
      },
      "icons": {
        "type": "set",
        "values": "Icon"
      }
    }
  },
  "store": {
    "iconGroups": {
      "type": "set",
      "values": "IconGroup"
    }
  },
  "seed": {
    "iconGroups": [
      {
        "id": "main",
        "name": "Main",
        "icons": []
      }
    ]
  }
}