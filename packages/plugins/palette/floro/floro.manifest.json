{
  "version": "0.0.41",
  "name": "palette",
  "displayName": "Palette",
  "description": "A plugin for managing color constants.",
  "icon": {
    "light": "./palette_icon.unselected.light.svg",
    "dark": "./palette_icon.unselected.dark.svg",
    "selected": {
      "light": "./palette_icon.selected.light.svg",
      "dark": "./palette_icon.selected.dark.svg"
    }
  },
  "imports": {},
  "types": {
    "Shade": {
      "id": {
        "isKey": true,
        "type": "string"
      },
      "name": {
        "type": "string"
      }
    },
    "Color": {
      "id": {
        "isKey": true,
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "colorShades": {
        "type": "set",
        "bounded": true,
        "values": "ColorShade"
      }
    },
    "ColorShade": {
      "id": {
        "isKey": true,
        "type": "ref<$.shades.values>"
      },
      "hexcode": {
        "nullable": true,
        "type": "string"
      }
    }
  },
  "store": {
    "shades": {
      "type": "set",
      "emptyable": false,
      "values": "Shade"
    },
    "colorPalettes": {
      "type": "set",
      "emptyable": false,
      "values": "Color"
    }
  },
  "seed": {
    "shades": [
      {
        "id": "light",
        "name": "Light"
      },
      {
        "id": "regular",
        "name": "Regular"
      },
      {
        "id": "dark",
        "name": "Dark"
      }
    ],
    "colorPalettes": [
      {
        "id": "white",
        "name": "White",
        "colorShades": [
          {
            "id": "$(palette).shades.id<light>"
          },
          {
            "hexcode": "#FFFFFF",
            "id": "$(palette).shades.id<regular>"
          },
          {
            "id": "$(palette).shades.id<dark>"
          }
        ]
      },
      {
        "id": "black",
        "name": "Black",
        "colorShades": [
          {
            "id": "$(palette).shades.id<light>"
          },
          {
            "hexcode": "#000000",
            "id": "$(palette).shades.id<regular>"
          },
          {
            "id": "$(palette).shades.id<dark>"
          }
        ]
      },
      {
        "id": "red",
        "name": "Red",
        "colorShades": [
          {
            "hexcode": "#F93D44",
            "id": "$(palette).shades.id<light>"
          },
          {
            "hexcode": "#CC2F35",
            "id": "$(palette).shades.id<regular>"
          },
          {
            "hexcode": "#AA2227",
            "id": "$(palette).shades.id<dark>"
          }
        ]
      },
      {
        "id": "green",
        "name": "Green",
        "colorShades": [
          {
            "hexcode": "#77F075",
            "id": "$(palette).shades.id<light>"
          },
          {
            "hexcode": "#3DD43A",
            "id": "$(palette).shades.id<regular>"
          },
          {
            "hexcode": "#26A324",
            "id": "$(palette).shades.id<dark>"
          }
        ]
      },
      {
        "id": "blue",
        "name": "Blue",
        "colorShades": [
          {
            "hexcode": "#7E91EC",
            "id": "$(palette).shades.id<light>"
          },
          {
            "hexcode": "#3D65DB",
            "id": "$(palette).shades.id<regular>"
          },
          {
            "hexcode": "#1F38B2",
            "id": "$(palette).shades.id<dark>"
          }
        ]
      }
    ]
  }
}
