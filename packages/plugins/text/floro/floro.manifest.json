{
  "version": "0.0.35",
  "name": "text",
  "displayName": "Text",
  "description": "A plugin for managing strings and translations.",
  "managedCopy": true,
  "icon": {
    "light": "./text.unselected.light.svg",
    "dark": "./text.unselected.dark.svg",
