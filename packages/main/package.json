{
  "name": "@floro/main",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env-shell  NODE_ENV=production node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm main.ts",
    "start:debug": "cross-env-shell NODE_ENV=production node --inspect-brk --experimental-specifier-resolution=node --loader ts-node/esm main.ts",
    "dev": "cross-env-shell DOTENV_CONFIG_PATH=../../.env NODE_ENV=development nodemon -e ts --watch ../ --ignore ../floro-desktop --experimental-specifier-resolution=node main.ts",
    "dev:nowatch": "cross-env-shell DOTENV_CONFIG_PATH=../../.env NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm main.ts",
    "dev:debug": "cross-env-shell DOTENV_CONFIG_PATH=../../.env NODE_ENV=development node --inspect-brk --experimental-specifier-resolution=node --loader ts-node/esm main.ts",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:ssr": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build": "npm run build:ssr && npm run build:client",
    "preview": "vite preview",
    "typecheck": "tsc --incremental main.ts --tsBuildInfoFile tsconfig.tsbuildinfo --isolatedModules --experimentalDecorators"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@floro/styles": "1.0.0",
    "apollo-server": "^3.10.1",
    "apollo-server-express": "^3.10.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "graphql-ws": "^5.11.2",
    "http-proxy-middleware": "^2.0.6",
    "inversify": "^6.0.1",
    "isomorphic-fetch": "^3.0.0",
    "js-cookie": "^3.0.1",
    "kill-port": "^2.0.1",
    "maildev": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^14.0.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "string-hex": "^1.0.0",
    "subscriptions-transport-ws": "^0.11.0",
    "text-encoding-shim": "^1.0.5",
    "ts-node": "^10.9.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^4.4.9"
  }
}
