{
  "name": "@floro/main-web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tsc": "tsc --build",
    "clean": "rimraf build",
    "build": "yarn clean && yarn tsc",
    "start": "NODE_ENV=production node .",
    "start:debug": "NODE_ENV=production node --inspect-brk .",
    "dev": "NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm main.ts",
    "dev:debug": "NODE_ENV=development node --inspect-brk --experimental-specifier-resolution=node --loader ts-node/esm main.ts",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:ssr": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@floro/styles": "1.0.0",
    "apollo-server": "^3.10.1",
    "apollo-server-express": "^3.10.1",
    "inversify": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
