{
  "name": "@floro/database",
  "version": "1.0.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc --build",
    "clean": "rimraf build tsconfig.tsbuildinfo",
    "build": "yarn clean && yarn tsc",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "test:inspect": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "typeorm": "npx typeorm-ts-node-commonjs --dataSource src/datasource.ts",
    "typeorm:migrate": "yarn typeorm migration:run",
    "typeorm:generate": "yarn typeorm migration:generate src/migrations/$NAME --timestamp",
    "typeorm:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "class-validator": "^0.13.2",
    "inversify": "^6.0.1",
    "jest": "^28.1.3",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^28.0.7",
    "typeorm": "^0.3.7",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typeorm-fixtures-cli": "^3.0.0",
    "typescript": "^4.3.5"
  }
}
